{
	"compilerOptions": {
		"outDir": "dist",
		"module": "commonjs",
		"target": "es2019",
		"esModuleInterop": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"rootDir": "src",
		"baseUrl": "src",
		"paths": {
			"@/*": ["*"]
		}
	},
	"include": ["src"],
	"exclude": []
}
